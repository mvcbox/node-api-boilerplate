{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "es5",
      "es6",
      "ES2017"
    ],
    "rootDir": "src",
    "outDir": "dist",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "removeComments": true,
    "declaration": false,
    "sourceMap": false,
    "inlineSources": false,
    "allowJs": false,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "incremental": false,
    "typeRoots": [
      "types",
      "node_modules/@types"
    ],
    "plugins": [
      {
        "transform": "typia/lib/transform"
      }
    ]
  },
  "include": [
    "src"
  ]
}